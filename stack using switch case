#include<bits/stdc++.h>
#define max 2
  

using namespace std;
class Stack{
    private:
    int st_arr[max];
    int data;
    int top=-1;
   
    public:
    void push(int data);
    void print();
};
void Stack::push(int data){
    if(top==max-1){
        cout<<"stackoverflow";
        return;
    }
    else
    {
    top=top+1;
    st_arr[top]=data;
    }
}
void Stack::print(){
    for(int i=top;i>=0;i--){
        cout<<st_arr[i]<<" ";
    }
}
int main(){
    Stack st;
    int data;
    int n;
    while(1){
 cout<<"\n1.push"<<endl;
 cout<<"2.print"<<endl;
 cout<<"3.exit"<<endl;
 cout<<"\nEnter your option :";
 cin>>n;
 switch(n){
     case 1: {
         cout<<"Enter element :";
        cin>>data;
        st.push(data);
     }break;
     case 2:{
         st.print();
     }break;
     case 3:{
         exit(1);
     }
 }
    }
    return 0;
}
